<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="huntermahroug.com.lille1campus">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".LilleCampusApplication_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_univ_lille"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".view.MainActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.EventDetailsActivity"></activity>
    </application>

</manifest>